{
  "name": "valcommerce",
  "version": "1.0.0",
  "description": "This is a ecommerce app that allow users post a product for sale and get list of products.",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run build:prod",
    "start:dev": "nodemon ./server/app.js --exec babel-node",
    "start": "node dist/app.js",
    "clean:dist": "rimraf ./dist",
    "build:prod": "npm run clean:dist && babel ./server --out-dir dist --copy-files",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "undo:migrations": "node_modules/.bin/sequelize db:migrate:undo:all",
    "migrations": "node_modules/.bin/sequelize db:migrate",
    "dev": "./node_modules/.bin/webpack-dev-server --config ./webpack.config.js",
    "start:concurrent": "concurrently \"npm run start:dev\" \"npm run dev\"",
    "build:client": "webpack --mode=production",
    "build": "npm run build:client && npm run build:prod",
    "heroku-postbuild": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/valentineezeh/valCommerce.git"
  },
  "author": "Valentine Ezeh",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/valentineezeh/valCommerce/issues"
  },
  "homepage": "https://github.com/valentineezeh/valCommerce#readme",
